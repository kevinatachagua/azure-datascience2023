{
	"name": "1-DataFlow_Geologia",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_trabajofinalexcel",
						"type": "DatasetReference"
					},
					"name": "geologia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_master_trabajofinal",
						"type": "DatasetReference"
					},
					"name": "sinkGeologia"
				}
			],
			"transformations": [
				{
					"name": "derivedColumngeologia"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> geologia",
				"geologia derive(Estimacion = toString(Estimacion),",
				"          Estatus = toString(Estatus),",
				"          Nombre_de_Estructura = toString(Nombre_de_Estructura),",
				"          Full_Year_Budget = toDouble(Full_Year_Budget),",
				"          Budget_YTD_April = toDouble(Budget_YTD_April),",
				"          Actual_YTD_April = toDouble(Actual_YTD_April),",
				"          Detalle = toString(Detalle),",
				"          Observacion = toString(Observacion),",
				"          ActualVSFullYearBudget = Actual_YTD_April-Full_Year_Budget,",
				"          ActualVSBudget = Actual_YTD_April-Budget_YTD_April) ~> derivedColumngeologia",
				"derivedColumngeologia sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'m_geologia.csv',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGeologia"
			]
		}
	}
}