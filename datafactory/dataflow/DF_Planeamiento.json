{
	"name": "DF_Planeamiento",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_trabajofinalexcel",
						"type": "DatasetReference"
					},
					"name": "planeamiento"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_master_trabajofinal",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Estimacion as string,",
				"          Estatus as string,",
				"          Nombre_de_Estructura as string,",
				"          Full_Year_Budget as double,",
				"          Budget_YTD_April as double,",
				"          Actual_YTD_April as double,",
				"          Detalle as string,",
				"          Observacion as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> planeamiento",
				"planeamiento derive(Estimacion = toString(Estimacion),",
				"          Estatus = toString(Estatus),",
				"          Nombre_de_Estructura = toString(Nombre_de_Estructura),",
				"          Full_Year_Budget = toDouble(Full_Year_Budget),",
				"          Budget_YTD_April = toDouble(Budget_YTD_April),",
				"          Actual_YTD_April = toDouble(Actual_YTD_April),",
				"          Detalle = toString(Detalle),",
				"          Observacion = toString(Observacion),",
				"          ActualVSFullYearBudget = Actual_YTD_April-Full_Year_Budget,",
				"          ActualVSBudget = Actual_YTD_April-Budget_YTD_April) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'m_planeamiento.csv',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}